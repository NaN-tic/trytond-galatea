<form>
    <label name="name"/>
    <field name="name"/>
    <label name="uri"/>
    <field name="uri" widget="url"/>
    <label name="folder"/>
    <field name="folder"/>
    <label name="static_folder"/>
    <field name="static_folder"/>
    <label name="active"/>
    <field name="active"/>
    <label name="company"/>
    <field name="company"/>
    <label name="currency"/>
    <field name="currency"/>
    <label name="timezone"/>
    <field name="timezone"/>
    <notebook colspan="4">
        <page id="configuration" string="Configuration">
            <label name="registration"/>
            <field name="registration"/>
            <label name="logo"/>
            <field name="logo"/>
        </page>
        <page id="seo" string="SEO">
            <label name="metadescription"/>
            <field name="metadescription"/>
            <label name="metakeyword"/>
            <field name="metakeyword"/>
            <label name="metatitle"/>
            <field name="metatitle"/>
        </page>
        <page id="email" string="Email">
            <label name="smtp_server"/>
            <field name="smtp_server"/>
        </page>
        <page id="countries" string="Countries">
            <label name="country"/>
            <field name="country"/>
            <field name="countries" colspan="4"/>
        </page>
        <page id="languages" string="Languages">
            <field name="languages" colspan="4"/>
        </page>
    </notebook>
    <group col="5" colspan="4" id="buttons">
        <button icon="tryton-cancel" name="remove_cache" string="Remove Cache"/>
    </group>
</form>
