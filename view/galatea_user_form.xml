<data>
    <xpath expr="/form" position="replace_attributes">
        <form string="Galatea User"/>
    </xpath>
    <xpath expr="/form/group[@id='checkboxes']" position="after">
        <pdb; label name="email"/>
        <field name="email"/>
        <label name="password"/>
        <field name="password" widget="password"/>
        <label name="active"/>
        <field name="active"/>
        <label name="manager"/>
        <field name="manager"/>
        <label name="company"/>
        <field name="company"/>
        <label name="timezone"/>
        <field name="timezone"/>
        <newline/>
        <separator name="websites" colspan="4"/>
        <field name="websites" colspan="4"/>
    </xpath>
</data>
