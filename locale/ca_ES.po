msgctxt "error:galatea.send.password:"
msgid "%s. New password reset"
msgstr "%s. Nova contrasenya"

msgctxt "error:galatea.send.password:"
msgid ""
"Hello %s\n"
"New password reset for your user account %s: %s\n"
"\n"
"%s\n"
"\n"
"You could drop this email.\n"
"Don't repply this email. It's was generated automatically"
msgstr ""
"Hola %s\n"
"Una nova contrasenya s'ha afegit al usuari %s: %s\n"
"\n"
"%s\n"
"\n"
"Elimina aquest correu.\n"
"No respongui aquest correu. S'ha generat automàticament."

msgctxt "error:galatea.send.password:"
msgid "Send new password to %s"
msgstr "Enviat nova contrasenya a %s"

msgctxt "error:galatea.static.file:"
msgid ""
"Invalid file name:\n"
"                (1) '..' in file name (OR)\n"
"                (2) file name contains '/'"
